{
  "name": "discord-sg",
  "version": "1.0.0-mvp",
  "description": "Discord bot",
  "main": "/core/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "build:start": "node -r ./tsconfig-paths-bootstrap.js build/src/core/index.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js"
  ],
  "author": "astisek@hotmail.com",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@mapbox/node-pre-gyp": "^2.0.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "ffmpeg-static": "^5.2.0",
    "pg": "^8.13.3",
    "pino": "^9.6.0",
    "prism-media": "^1.3.5",
    "reflect-metadata": "^0.2.2",
    "sodium": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "youtubei.js": "^13.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@discordjs/opus",
      "ffmpeg-static",
      "sodium"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.5",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "./*"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/core/index.ts",
    "ext": "ts"
  }
}
